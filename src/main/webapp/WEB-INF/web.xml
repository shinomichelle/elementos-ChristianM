<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>webota</display-name>
    <servlet>
        <servlet-name>Conejo</servlet-name>
        <servlet-class>carrillo.cuentas.controladores.ServletCrearCuenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Perro</servlet-name>
        <servlet-class>carrillo.cuentas.controladores.ServletRegistro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Conejo</servlet-name>
        <url-pattern>/Chapo.Guzman</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Perro</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
